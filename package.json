{
  "name": "@gocommerce/styleguide",
  "version": "5.4.1",
  "scripts": {
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "github": "gh-pages -d styleguide",
    "babel": "NODE_ENV=production babel ./react/components --out-dir ./lib --ignore '__tests__,*.spec.js' && npm run copycss",
    "compile": "run-s cleanlib babel",
    "cleanlib": "rm -rf lib",
    "entrypoints": "node ./scripts/entrypoints.js",
    "copycss": "copyfiles -u 2 react/**/*.css lib"
  },
  "homepage": "https://vtex-gocommerce.github.io/styleguide",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "copyfiles": "^1.2.0",
    "eslint-config-vtex": "^7.0.0",
    "eslint-config-vtex-react": "^3.0.3",
    "eslint-config-vtex-react-app": "^3.1.1",
    "fs": "0.0.1-security",
    "gh-pages": "^1.2.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-scripts": "^1.1.4",
    "react-styleguidist": "^6.5.3",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-free-brands": "^5.0.9",
    "@fortawesome/fontawesome-pro-light": "^5.0.9",
    "@fortawesome/fontawesome-pro-regular": "^5.0.9",
    "@fortawesome/fontawesome-pro-solid": "^5.0.9",
    "@fortawesome/react-fontawesome": "0.0.17",
    "@gocommerce/tachyons": "^3.0.12",
    "i18n-country-code": "^1.0.0",
    "npm": "^6.1.0",
    "react-input-mask": "^1.2.2",
    "react-onclickoutside": "^6.7.1",
    "react-paginate": "^5.2.2",
    "react-responsive-modal": "^2.0.1",
    "react-select": "^2.0.0-beta.2",
    "vtex": "^2.22.0",
    "vtex-tachyons": "2.5.1",
    "speakingurl": "^14.0.1",
    "npm-run-all": "^4.1.3"
  }
}
