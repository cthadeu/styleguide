{
  "name": "@gocommerce/styleguide",
  "version": "1.0.3",
  "scripts": {
    "test": "react-scripts test --env=jsdom",
    "predeploy": "npm run styleguide:build",
    "deploy": "gh-pages -d styleguide",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "compile": "npm run cleanlib && NODE_ENV=production babel --plugins transform-es2015-modules-commonjs ./src --out-dir ./lib --ignore '__tests__,*.spec.js' && npm run copycss",
    "copycss": "copyfiles -u 2 src/**/*.css lib",
    "cleanlib": "sudo rm -rf lib",
    "prepublishOnly": "npm run compile",
    "develop": "npm link && watch 'npm run compile' src"
  },
  "files": [
    "lib/"
  ],
  "homepage": "https://gocommerce.github.io/styleguide",
  "devDependencies": {
    "@fortawesome/fontawesome": "^1.1.3",
    "@fortawesome/fontawesome-free-brands": "^5.0.6",
    "@fortawesome/fontawesome-pro-light": "^5.0.6",
    "@fortawesome/fontawesome-pro-regular": "^5.0.6",
    "@fortawesome/fontawesome-pro-solid": "^5.0.6",
    "@fortawesome/react-fontawesome": "0.0.17",
    "@gocommerce/tachyons": "^1.0.2-gc.16",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "copyfiles": "^1.2.0",
    "eslint-config-vtex": "^7.0.0",
    "eslint-config-vtex-react": "^3.0.3",
    "eslint-config-vtex-react-app": "^3.1.1",
    "gh-pages": "^1.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.0.17",
    "react-styleguidist": "^6.2.5",
    "watch": "^1.0.2"
  }
}
